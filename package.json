{
  "name": "pdml",
  "displayName": "PDML",
  "description": "Practical Data & Markup Language support",
  "publisher": "shelby-landen",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/slanden/vscode-pdml.git"
  },
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "pdml",
    "practical",
    "markup",
    "xml"
  ],
  "main": "./client/out/extension",
  "contributes": {
    "languages": [
      {
        "id": "pdml",
        "aliases": [
          "Practical Data & Markup Language",
          "pdml"
        ],
        "extensions": [
          ".pdml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pdml",
        "scopeName": "source.pdml",
        "path": "./syntaxes/pdml.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.css": "css",
          "meta.embedded.block.javascript": "javascript"
        },
        "unbalancedBracketScopes": [
          "constant.character.escape"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm install && npm run compile",
    "compile": "tsc -b",
    "publish": "vsce publish",
    "watch": "tsc -b -w"
  },
  "devDependencies": {
    "@types/node": "^18.8.3",
    "typescript": "^4.8.4"
  }
}